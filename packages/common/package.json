{
  "name": "@videofly/common",
  "version": "0.1.0",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./resend": "./src/email.ts",
    "./MagicLinkEmail": "./src/emails/magic-link-email.tsx",
    "./subscriptions": "./src/subscriptions.ts",
    "./storage": "./src/storage.ts",
    "./config/credits": "./src/config/credits.ts",
    "./services/credit": "./src/services/credit.ts",
    "./services/video": "./src/services/video.ts",
    "./ai": "./src/ai/index.ts",
    "./utils/callback-signature": "./src/utils/callback-signature.ts",
    "./env": "./src/env.mjs"
  },
  "typesVersions": {
    "*": {
      "config/*": ["src/config/*"],
      "services/*": ["src/services/*"],
      "ai": ["src/ai/index.ts"],
      "utils/*": ["src/utils/*"],
      "*": ["src/*"]
    }
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check '**/*.{mjs,ts,json}' "
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@videofly/db": "workspace:*",
    "@videofly/ui": "workspace:*",
    "nanoid": "^5.0.7",
    "resend": "2.1.0"
  },
  "devDependencies": {
    "@videofly/eslint-config": "workspace:*",
    "@videofly/prettier-config": "workspace:*",
    "@videofly/typescript-config": "workspace:*",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@videofly/eslint-config/base"
    ]
  },
  "prettier": "@videofly/prettier-config"
}
